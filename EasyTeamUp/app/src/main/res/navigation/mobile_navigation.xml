<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_maps">

    <fragment
        android:id="@+id/navigation_maps"
        android:name="com.example.easy_team_up.MapsFragment"
        android:label="@string/title_maps"
        tools:layout="@layout/fragment_maps" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.easy_team_up.ListFragment"
        android:label="@string/title_list"
        tools:layout="@layout/fragment_list" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.easy_team_up.NotificationsFragment"
        android:label="@string/title_my_events"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_create_event"
        android:name="com.example.easy_team_up.CreateEvent"
        android:label="@string/title_my_events"
        tools:layout="@layout/fragment_notifications" />
</navigation>